{
  "name": "consolidation/site-aliases",
  "description": "Libraries for managing Drush aliases and contexts, used to manage and configure scripting options for local and remote sites.",
  "homepage": "http://www.drush.org",
  "license": "GPL-2.0+",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "authors": [
    { "name": "Greg Anderson", "email": "greg.1.anderson@greenknowe.org" },
  ],
  "require": {
    "php": ">=5.4.5",
    "symfony/yaml": "~2.2",
    "symfony/var-dumper": "^2.6.3"
  },
  "require-dev": {
    "phpunit/phpunit": ">=3.5"
  },
  "autoload": {
    "psr-0": {
      "Drush":        "lib/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Unish":        "tests/"
    }
  },
  "extra": {
    "branch-alias": {
        "dev-master": "1.0.x-dev"
    }
  }
}
